IDR_MENU1 MENU
BEGIN
    POPUP "&Help"
    BEGIN
        MENUITEM "Open Reference Manual",                    ID_HELP_MANUAL
        MENUITEM "Open Macro Prompting Guide",               ID_ITEM4
        MENUITEM "About Macro Editor",                       ID_HELP_ABOUT
    END
END

IDR_MENU1 RCDATA
BEGIN
ID_ITEM1,1,0
0
END

